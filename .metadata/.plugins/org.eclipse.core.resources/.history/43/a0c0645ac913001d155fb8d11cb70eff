package com.itteam.prueba.entity;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

@Entity

public class Offerse {
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	private int offerse_id;
	private float price_value;
	private String description;
	private String name;
	
	
	public Offerse() {
		super();
	}


	public Offerse(int offerse_id, float price_value, String description, String name) {
		super();
		this.offerse_id = offerse_id;
		this.price_value = price_value;
		this.description = description;
		this.name = name;
	}
	

}
